{
  "non-interactive": false,
  "dry-run": false,
  "verbose": false,
  "force": false,
  "pkgFiles": [
    "package.json"
  ],
  "increment": "patch",
  "preReleaseId": null,
  "buildCommand": false,
  "safeBump": true,
  "beforeChangelogCommand": "conventional-changelog -p angular -i CHANGELOG.md -s",
  "changelogCommand": "conventional-changelog -p angular | tail -n +3",
  "requireCleanWorkingDir": false,
  "src": {
    "commit": true,
    "commitMessage": "Release %s",
    "tag": true,
    "tagName": "%s",
    "tagAnnotation": "Release %s",
    "push": true,
    "pushRepo": null,
    "beforeStartCommand": false,
    "afterReleaseCommand": false
  },
  "npm": {
    "publish": false,
    "publishPath": ".",
    "tag": "latest",
    "private": false,
    "access": null
  },
  "github": {
    "release": false,
    "releaseName": "SubjectsPlus %s",
    "preRelease": false,
    "draft": false,
    "tokenRef": "GITHUB_TOKEN",
    "assets": null,
    "host": null
  },
  "dist": {
    "repo": false,
    "stageDir": ".stage",
    "baseDir": "dist",
    "files": [
      "**/*"
    ],
    "pkgFiles": null,
    "commit": true,
    "commitMessage": "Release %s",
    "tag": true,
    "tagName": "%s",
    "tagAnnotation": "Release %s",
    "push": true,
    "beforeStageCommand": false,
    "afterReleaseCommand": false,
    "github": {
      "release": false
    },
    "npm": {
      "publish": false
    }
  }
}
